{
  "env": "dev",
  "vars": {
    "taskset": "0,1",
    "data": "/mnt/data/equities.us1-dev"
  },
  "adapters": {
    "market_data": "massive",
    "execution": "alpaca-dev"
  },
  "processes": {
    "tp1": { "args": ["-t", "500"] },
    "wdb1": { "args": ["write_every", "00:15"] },
    "hdb1" : {}
  },
  "secrets": {
    "alpaca.api_key": "${env:ALPACA_API_KEY_DEV}",
    "alpaca.secret_key": "${env:ALPACA_SECRET_DEV}"
  }
}