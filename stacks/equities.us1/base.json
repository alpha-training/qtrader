{
  "version": "1.1",
  "stack": "equities.us1",
  "description": "US equities feed and backtesting stack",
  "base_port": 5000,

  "vars": {
    "qbin": "${env:QHOME}/${.z.o}/q",
    "data": "${env:DATA}/equities.us1",
    "cmd": "nohup ${qbin}",
    "taskset": ""
  },

  "processes": {
    "tp1": {
      "proc": "tp",
      "port_offset": 10,
      "args": ["-t", "1000"],
      "resources": { "cpu": "1,2" },
      "depends_on": []
    },

    "rdb1": {
      "proc": "rdb",
      "port_offset": 11,
      "depends_on": ["tp1"]
    },

    "wdb1": {
      "proc": "wdb",
      "port_offset": 12,
      "args": ["write_every", "00:30"],
      "depends_on": ["tp1"]
    },

    "c2": {
      "proc": "c2",
      "port_offset": 20,
      "depends_on": ["tp1"]
    }
  },

  "adapters": {
    "market_data": "yahoo",
    "execution": "alpaca"
  },

  "metadata": {
    "owner": "alpha",
    "created": "2025-11-12"
  }
}